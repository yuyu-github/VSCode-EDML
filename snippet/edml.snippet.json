{
  "if": {
    "prefix": "if",
    "body": [
      "if (${1:condition}) ${2:expression}"
    ]
  },
  "ifelse": {
    "prefix": "ifelse",
    "body": [
      "if (${1:condition}) ${2:expression} else ${3:expression}"
    ]
  },
  "switch": {
    "prefix": "switch",
    "body": [
      "switch (${1:value}) case (${2:pattern}) ${3:expression} default ${4:expression}"
    ]
  },
  "trycatch": {
    "prefix": "trycatch",
    "body": [
      "try ${1:expression} catch (${2:error}) ${3:expression}"
    ]
  },
  "for": {
    "prefix": "for",
    "body": [
      "for (${1:item} in ${2:object}) ${3:expression}"
    ]
  },
  "while": {
    "prefix": "while",
    "body": [
      "while (${1:condition}) ${2:expression}"
    ]
  },
  "dowhile": {
    "prefix": "dowhile",
    "body": [
      "do ${1:expression} while (${2:condition})"
    ]
  },
  "throw": {
    "prefix": "throw",
    "body": [
      "throw exception(${1:name}, ${2:message});"
    ]
  },
  "require": {
    "prefix": "require",
    "body": [
      "require (${1:vars}) ${2:expression}"
    ]
  }
}
